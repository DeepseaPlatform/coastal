<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE configuration PUBLIC "-//DEEPSEA//COASTAL configuration//EN" "https://deepseaplatform.github.io/coastal/coastal.dtd">
<configuration>
	<coastal>
		<main>tests.strings.CharAt01</main>
		<target>tests.strings</target>
		<trigger>tests.strings.CharAt01.test(S: String, X: int)</trigger>
		<delegate>
			<target>java.lang.String</target>
			<model>za.ac.sun.cs.coastal.model.String</model>
